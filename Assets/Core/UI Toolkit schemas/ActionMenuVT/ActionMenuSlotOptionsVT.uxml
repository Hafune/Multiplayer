                    <ui:VisualElement name="NoRune" class="rune-item">
                        <ui:VisualElement class="rune-icon no-rune" style="background-image: url(&quot;project://database/Assets/Prefabs/Entities/Warrior/runes.png?fileID=665348599&amp;guid=d45ac0a4ce799c1439b13863e05156c8&amp;type=3#runes_5&quot;);" />
